<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mapsbasics2"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-feature android:glEsVersion="0x00030000" android:required="true"/>
    <uses-feature android:name="android.hardware.camera.ar" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.MapsBasics2"
        tools:targetApi="31">
        <meta-data android:name="com.google.ar.core" android:value="required"/>
        <activity
            android:name=".ARCementerio"
            android:exported="false" />
        <activity
            android:name=".obituario.ObituarioActivity"
            android:exported="false" />
        <activity
            android:name=".obituario.ActivityLista"
            android:exported="false" />
        <activity
            android:name=".artistForm.FormActivity"
            android:exported="false" />
        <activity
            android:name=".artistMap.ArtistMapActivity"
            android:exported="false" />
        <activity
            android:name=".topics.IlustresActivity"
            android:exported="false" />
        <activity
            android:name=".topics.InfoAppActivity"
            android:exported="false" />
        <activity
            android:name=".topics.NewsActivity"
            android:exported="false" />
        <activity
            android:name=".actividades.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
        <activity
            android:name=".topics.CameraViewActivity"
            android:exported="false" />
        <activity
            android:name=".topics.PanoramaActivity"
            android:exported="false" />
        <activity
            android:name=".topics.NewProcessorActivity"
            android:exported="false" />
        <activity
            android:name=".topics.FirstMapActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="${MAPS_API_KEY}" />
        <meta-data
            android:name="realidadprobando2.UnityActivity"
            android:value="false" />
    </application>

</manifest>